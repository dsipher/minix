size_t fwrite(const void *ptr, size_t size, size_t nmemb, FILE *fp)
{
    const unsigned char *cp = ptr;
    size_t s;
    size_t ndone = 0;

    if (size)
        while (ndone < nmemb) {
            s = size;

            do {
                if ((--(fp)->_count >= 0 ? (int) (*(fp)->_ptr++ = (*cp)) : __flushbuf((*cp),(fp))) == (-1))
                    return ndone;

                cp++;
            } while (--s);

            ndone++;
        }

    return ndone;
}
