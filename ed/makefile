##############################################################################
#
#                                                           ux/64 line editor
#
##############################################################################

all:	ed

clean:
	rm -f ed *.o

HDRS=	ed.h tools.h
OBJS=	ed.o

ed.o: 		ed.c $(HDRS)

ed: $(OBJS)
	$(CC) $(CFLAGS) -o ed $(OBJS)

